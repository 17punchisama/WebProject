<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebProject</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="@Url.Content("~/css/" + ViewData["Style"] + ".css")" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans+Thai:wght@100;200;300;400;500;600;700&display=swap"
          rel="stylesheet">
</head>
<body>
     @RenderBody()
    <script src="@Url.Content("~/js/" + ViewData["Script"] + ".js")" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
